  }
  {
    'include': '#string_quoted_double'
  }
  {
    'include': '#dotted_name'
  }
  {
    'begin': '(\\()'
    'end': '(\\))'
    'patterns': [
      {
        'include': '$self'
      }
    ]
  }
  {
    'captures':
      '1':
        'name': 'punctuation.definition.list.begin.gap'
      '2':
        'name': 'meta.empty-list.gap'
      '3':
        'name': 'punctuation.definition.list.end.gap'
    'match': '(\\[)(\\s*(\\]))\\b'
