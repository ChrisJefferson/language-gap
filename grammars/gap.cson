          '1':
            'name': 'punctuation.separator.list.gap'
        'patterns': [
          {
            'include': '$self'
          }
        ]
      }
    ]
  }
  {
    'captures':
      '1':
        'name': 'punctuation.definition.tuple.begin.gap'
      '2':
        'name': 'meta.empty-tuple.gap'
      '3':
        'name': 'punctuation.definition.tuple.end.gap'
    'match': '(\\()(\\s*(\\)))'
    'name': 'meta.structure.tuple.gap'
  }
  {
    'captures':
      '1':
        'name': 'punctuation.definition.dictionary.begin.gap'
      '2':
