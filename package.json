{
    "name": "gap",
    "displayName": "GAP",
    "description": "GAP Language",
    "version": "0.0.2",
    "publisher": "gap",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gap",
            "aliases": ["GAP", "gap"],
            "extensions": [".g",".gi",".gd","..gaprc"],
            "configuration": "./gap.configuration.json"
        }],
        "grammars": [{
            "language": "gap",
            "scopeName": "source.gap",
            "path": "./syntaxes/gap.tmLanguage"
        }]
    },
    "repository" :
  { "type" : "git"
  , "url" : "https://github.com/ChrisJefferson/language-gap.git"
  }
}